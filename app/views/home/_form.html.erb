<%= form_for @sample, as: :sample, url: get_prediction_home_index_path(@sample), remote: true do |f| %>
  <%= f.label 'Height (ft.)' %>
  <%= f.number_field :height, in: 1..10 %>
  <%= f.label 'Weight (lbs.)' %>
  <%= f.number_field :weight, in: 1..1500 %>
  <%= f.submit "Get Prediction" %>
<% end %>